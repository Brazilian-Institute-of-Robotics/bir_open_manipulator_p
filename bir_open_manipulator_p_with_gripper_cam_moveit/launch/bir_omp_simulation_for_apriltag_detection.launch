<launch>
    <!-- Launch Gazebo -->
    <include file="$(find bir_open_manipulator_p_with_gripper_cam_moveit)/launch/bir_omp_gazebo_with_apriltags.launch"/>
    <!-- Launch MoveIt! -->
    <include file="$(find bir_open_manipulator_p_with_gripper_cam_moveit)/launch/bir_omp_moveit_planning_execution.launch"/>
    <!-- Launch Tag Detector -->
    <include file="$(find apriltag_ros)/launch/continuous_detection.launch">
        <arg name="camera_name" value="/open_manipulator_p/camera1/" />
        <arg name="image_topic" default="image_raw" />
        <arg name="camera_frame" value="camera1" />
    </include>
    
    <!-- Apriltag Detection image view :: Some computers just doesn't work image_view -->
    <!--<node name="apriltag_image_view" pkg="image_view" type="image_view" output="screen">
        <remap from="/image" to="/tag_detections_image"/>
    </node>-->

</launch>
